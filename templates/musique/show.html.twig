{% extends 'base.html.twig' %}

{% block title %}Musique{% endblock %}

{% block body %}
    <h1>Musique</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ musique.id }}</td>
            </tr>
            <tr>
                <th>Titre</th>
                <td>{{ musique.titre }}</td>
            </tr>
            <tr>
                <th>Date</th>
                <td>{{ musique.date ? musique.date|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Link</th>
                <td>{{ musique.link }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('musique_index') }}">back to list</a>

    <a href="{{ path('musique_edit', {'id': musique.id}) }}">edit</a>

    {{ include('musique/_delete_form.html.twig') }}
{% endblock %}
